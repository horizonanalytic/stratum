{
  "Function": {
    "prefix": "fx",
    "body": [
      "fx ${1:name}(${2:params}) {",
      "\t$0",
      "}"
    ],
    "description": "Define a function"
  },
  "Function with return type": {
    "prefix": "fxr",
    "body": [
      "fx ${1:name}(${2:params}) -> ${3:ReturnType} {",
      "\t$0",
      "}"
    ],
    "description": "Define a function with explicit return type"
  },
  "Async function": {
    "prefix": "fxa",
    "body": [
      "async fx ${1:name}(${2:params}) {",
      "\t$0",
      "}"
    ],
    "description": "Define an async function"
  },
  "Let binding": {
    "prefix": "let",
    "body": "let ${1:name} = ${0:value}",
    "description": "Variable declaration"
  },
  "Let with type": {
    "prefix": "lett",
    "body": "let ${1:name}: ${2:Type} = ${0:value}",
    "description": "Variable declaration with explicit type"
  },
  "If statement": {
    "prefix": "if",
    "body": [
      "if ${1:condition} {",
      "\t$0",
      "}"
    ],
    "description": "If statement"
  },
  "If-else statement": {
    "prefix": "ife",
    "body": [
      "if ${1:condition} {",
      "\t$2",
      "} else {",
      "\t$0",
      "}"
    ],
    "description": "If-else statement"
  },
  "For loop": {
    "prefix": "for",
    "body": [
      "for ${1:item} in ${2:collection} {",
      "\t$0",
      "}"
    ],
    "description": "For-in loop"
  },
  "For range": {
    "prefix": "forr",
    "body": [
      "for ${1:i} in ${2:0}..${3:10} {",
      "\t$0",
      "}"
    ],
    "description": "For loop with range"
  },
  "While loop": {
    "prefix": "while",
    "body": [
      "while ${1:condition} {",
      "\t$0",
      "}"
    ],
    "description": "While loop"
  },
  "Match expression": {
    "prefix": "match",
    "body": [
      "match ${1:value} {",
      "\t${2:pattern} => ${3:expression},",
      "\t_ => ${0:default}",
      "}"
    ],
    "description": "Match expression"
  },
  "Struct definition": {
    "prefix": "struct",
    "body": [
      "struct ${1:Name} {",
      "\t${2:field}: ${3:Type},$0",
      "}"
    ],
    "description": "Define a struct"
  },
  "Enum definition": {
    "prefix": "enum",
    "body": [
      "enum ${1:Name} {",
      "\t${2:Variant1},",
      "\t${0:Variant2}",
      "}"
    ],
    "description": "Define an enum"
  },
  "Interface definition": {
    "prefix": "interface",
    "body": [
      "interface ${1:Name} {",
      "\tfx ${2:method}(${3:params})$0",
      "}"
    ],
    "description": "Define an interface"
  },
  "Impl block": {
    "prefix": "impl",
    "body": [
      "impl ${1:Type} {",
      "\t$0",
      "}"
    ],
    "description": "Implementation block"
  },
  "Impl for interface": {
    "prefix": "implfor",
    "body": [
      "impl ${1:Interface} for ${2:Type} {",
      "\t$0",
      "}"
    ],
    "description": "Implement interface for type"
  },
  "Try-catch": {
    "prefix": "try",
    "body": [
      "try {",
      "\t$1",
      "} catch {",
      "\t$0",
      "}"
    ],
    "description": "Try-catch block"
  },
  "Closure": {
    "prefix": "cl",
    "body": "|${1:params}| { $0 }",
    "description": "Closure expression"
  },
  "Closure with type": {
    "prefix": "clt",
    "body": "|${1:x}: ${2:Type}| -> ${3:ReturnType} { $0 }",
    "description": "Closure with type annotations"
  },
  "Test function": {
    "prefix": "test",
    "body": [
      "#[test]",
      "fx ${1:test_name}() {",
      "\t$0",
      "}"
    ],
    "description": "Test function"
  },
  "Assert": {
    "prefix": "assert",
    "body": "assert($1)",
    "description": "Assert statement"
  },
  "Assert equals": {
    "prefix": "asserteq",
    "body": "assert_eq($1, $2)",
    "description": "Assert equals"
  },
  "Print": {
    "prefix": "print",
    "body": "print($0)",
    "description": "Print to stdout"
  },
  "Println": {
    "prefix": "println",
    "body": "println($0)",
    "description": "Print line to stdout"
  },
  "Pipeline": {
    "prefix": "pipe",
    "body": [
      "${1:data}",
      "\t|> ${2:transform}",
      "\t|> ${0:result}"
    ],
    "description": "Pipeline expression"
  },
  "Import": {
    "prefix": "import",
    "body": "import ${1:module}",
    "description": "Import statement"
  },
  "Doc comment": {
    "prefix": "///",
    "body": [
      "/// ${1:Description}",
      "///",
      "/// ## Arguments",
      "/// - `${2:param}`: ${3:Description}",
      "///",
      "/// ## Returns",
      "/// ${0:Return value description}"
    ],
    "description": "Documentation comment"
  },
  "Null coalescing": {
    "prefix": "??",
    "body": "${1:nullable} ?? ${0:default}",
    "description": "Null coalescing operator"
  },
  "Optional chaining": {
    "prefix": "?.",
    "body": "${1:obj}?.${0:field}",
    "description": "Optional chaining"
  }
}
